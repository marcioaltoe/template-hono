# Application
NODE_ENV=development
PORT=3000

# Database
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=hono_db
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_SSL=false

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# Security (dev keys only!)
JWT_SECRET=dev_jwt_secret_key_at_least_32_characters_long
ENCRYPTION_KEY=dev_encryption_key_at_least_32_chars_long
API_KEY_SALT=dev_salt_16_chars

# Logging
LOG_LEVEL=debug
LOG_FORMAT=pretty

# Feature Flags
ENABLE_CORS=true
ENABLE_RATE_LIMITING=false
ENABLE_API_DOCS=true