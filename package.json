{
  "private": true,
  "name": "template-hono",
  "version": "0.1.0",
  "type": "module",
  "module": "main.ts",
  "scripts": {
    "clean": "rm -rf dist node_modules coverage bun.lockb bun.lock",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push --force",
    "db:studio": "drizzle-kit studio",
    "dev": "bun run --hot src/main.ts",
    "format": "biome format --write . && bun run format:md && bun run format:pkg",
    "format:md": "prettier --write '**/*.md' --log-level error",
    "format:pkg": "prettier-package-json --write package.json --log-level error",
    "lint": "biome check --write .",
    "lint:fix": "biome check --write . --unsafe",
    "prepare": "husky",
    "start": "bun run src/main.ts",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@hono/swagger-ui": "^0.5.2",
    "@hono/typebox-validator": "^0.3.3",
    "@sinclair/typebox": "^0.34.40",
    "bullmq": "^5.58.0",
    "drizzle-orm": "^0.44.4",
    "hono": "^4.9.2",
    "ioredis": "^5.7.0",
    "postgres": "^3.4.7",
    "ulid": "^3.0.1",
    "winston": "^3.17.0"
  },
  "peerDependencies": {
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/bun": "^1.2.20",
    "@types/ioredis": "^5.0.0",
    "@types/node": "24.3.0",
    "@vitest/coverage-v8": "^3.2.4",
    "dotenv": "^17.2.1",
    "drizzle-kit": "^0.31.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "prettier-package-json": "^2.8.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
